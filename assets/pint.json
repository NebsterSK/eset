{
  "preset": "laravel",
  "rules": {
    "concat_space": {
      "spacing": "one"
    },
    "class_attributes_separation": {
      "elements": {
        "const": "none",
        "method": "one",
        "property": "none",
        "trait_import": "none"
      }
    },
    "single_trait_insert_per_statement": true
  }
}