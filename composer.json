{
    "name": "nebstersk/eset",
    "description": "Common assets for LWS projects.",
    "keywords": [
        "NebsterSK",
        "laravel",
        "eset"
    ],
    "homepage": "https://github.com/nebstersk/eset",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukáš Neuschl",
            "email": "lukasneuschl@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NebsterSK\\Eset\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NebsterSK\\Eset\\EsetServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
